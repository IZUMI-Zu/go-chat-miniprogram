/* 容器样式 */
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100vh;
  background-color: #f0f0f0;
  padding: 20rpx;
}

.head {
  direction: ltr;
  font-synthesis: style;
  box-sizing: border-box;
  margin: 0;
  overflow-wrap: break-word;
  position: relative;
  font-weight: 600;
  outline: none;
  letter-spacing: -.02em;
  line-height: 40px;
  font-size: 28px;
  margin-bottom: 30%;
}

.output {
  --vp-custom-block-font-size: 14px;
  --vp-custom-block-tip-border: rgba(12, 124, 108, .5);
  --vp-custom-block-tip-text: rgb(12, 124, 108);
  --vp-custom-block-tip-bg: rgba(18, 181, 157, 0.048);
  font-weight: 400;
  direction: ltr;
  font-synthesis: style;
  box-sizing: border-box;
  border: 1px solid transparent;
  border-radius: 8px;
  padding: 16px 16px 8px;
  line-height: 24px;
  font-size: var(--vp-custom-block-font-size);
  border-color: var(--vp-custom-block-tip-border);
  color: var(--vp-custom-block-tip-text);
  background-color: var(--vp-custom-block-tip-bg);
  margin: 8px 0;
  width: 100%;
  height: 10vh;
}

.help {
